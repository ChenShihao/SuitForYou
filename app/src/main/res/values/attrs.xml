<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundImageView">
        <attr name="border_thickness" format="dimension" />
        <attr name="border_inside_color" format="color" />
        <attr name="border_outside_color" format="color" />
    </declare-styleable>

    <declare-styleable name="NumberPickView">
        <attr name="item_num" format="integer" />
        <attr name="item_max" format="integer" />
        <attr name="item_price" format="float" />
    </declare-styleable>

    <declare-styleable name="NumberPickMiniView">
        <attr name="mini_item_num" format="integer" />
        <attr name="mini_item_max" format="integer" />
        <attr name="mini_item_price" format="float" />
    </declare-styleable>
</resources>
